<div class="container">
  <div>
    <h2>Wix Shops</h2>
  </div
  <div>
    <div class="right-buttons">
      <%= link_to "Connect to Wix", '/auth/wix', class: "button" %>
    </div>
  </div
</div>

<table>
  <tr>
    <th> Name</th>
    <th> Created Date </th>
  </tr>
  <% @shops.each do |shop| %>
    <tr>
      <td> <%= link_to shop.name, shop_path(shop) %> </td>
      <td> <%= shop.created_at %> </td>
    </tr>
  <% end %>
</table>
